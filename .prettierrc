{
    "printWidth": 120,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "trailingComma": "all",
    "arrowParens": "always",
    "endOfLine": "lf",
    "bracketSpacing": true,
    "overrides": [
        {
            "files": "*.mscss",
            "options": {
                "parser": "scss"
            }
        }
    ]
}
